generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model StudentOverlay {
    uid String @unique // ITS insists this is unique now
    firstName String
    lastName String 
    pronouns String
    photoPath String
    hideProfile Boolean @default(false)
    hideDorm Boolean @default(false)
    hidePhoto Boolean @default(false)
}
